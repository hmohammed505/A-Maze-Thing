--gamecode
---- modesetters -----
 
function titleinit()
mode = "title"
end
 
function gameinit()
mode = "game"
 
w=128 -- width of the game map
h=128 -- height of the game map
 
-- collide with map tiles
player.cm=true
-- collide with world bounds
player.cw=true
 
 
--camera
cam_x=0
cam_y=0
 
--start music from pattern 0
music(0)
end
 
function leveloverinit()
 mode="levelover"
end

function gameoverinit()
mode = "gameover"
end

function deathinit()
mode = "dead"
end


 ---- updatefunctions ----
 
 
function titleupdate()
cls()
if btn(4,0) then
gameinit()
end
 
end
 
function gameupdate()
cls()
solid = solid_tile(player.x,player.y)
chest = chest_tile(player.x,player.y)
die = death()
if(solid == true)then
leveloverinit()
elseif(chest == true)then
gameoverinit() -- create function
elseif(die == true)then
deathinit()
end
end

function updategameover() -----change this
 rectfill(0,60,128,75,0)
 print("you have won!",player.x - 26, player.y + 12, 6)
 print("congratz!",player.x - 18 , player.y , 7)
end

function updatelevelover()
 if btnp(4) then
  nextlevel()
 end
end
 
 function deathupdate()
 
  rectfill(0,60,128,75,0)
 print("press start to retry.",player.x - 32 , player.y + 12 , 7)
 print("you have died!",player.x - 18, player.y, 6)
 
 if btnp(4) then
	currentlevel()
 end
 
 end
 
 function currentlevel()
 mode = "game"
 
 if(levelnum == 1) then
player.x=14
player.y=10
 
elseif(levelnum == 2) then
 
player.x= 280
player.y= 56
 
elseif(levelnum == 3) then
 
player.x= 544
player.y= 56
 
  
elseif(levelnum == 4) then
 
player.x= 113*8
player.y= 8
 
 
 end
 
 end
 
 
function _update()
 t=(t+1)%s --tick fwd
 if (t==0) then f=f%#sp+1
 end
 
 
if (mode == "title") then
    titleupdate()
  elseif (mode == "game") then
    gameupdate()
  elseif (mode == "levelover") then
    updatelevelover()
	elseif (mode == "gameover") then
    updategameover()
	elseif (mode == "dead")then
	deathupdate()
  end
 
end
 
---- nextlevel ------
 
function nextlevel()
 
gameinit()
levelnum += 1
 
if(levelnum == 1) then
player.x=14
player.y=10
 
elseif(levelnum == 2) then
 
player.x= 280
player.y= 56
 
elseif(levelnum == 3) then
 
player.x= 544
player.y= 56
 
  
elseif(levelnum == 4) then
 
player.x= 113*8
player.y= 8
 
end
 
end
 
----------- draw ----

function gamedraw()
cls()
rectfill(1,1,239,248,6)
map(0,0,0,0,32,32)
spr(sp[f],240,48)
spr(sp[f],240,56)

if(levelnum == 2) then
 
rectfill(265,1,504,248,1)
map(0,0,0,0,64,32)  
spr(sps[f],50*8,31*8)
spr(sps[f],51*8,31*8)

elseif(levelnum == 3) then
 
map(0,0,0,0,96,32)  
spr(sp[f],94*8,14*8)
spr(sp[f],94*8,13*8)

elseif(levelnum == 4) then
 
map(0,0,0,0,128,32)  


end
 
move(player)
movey(player)


	---camera
camera(player.x-64,player.y-64)

 ---drawing player
spr(2,player.x,player.y)
	
	enemydraw()
     death()
 
end
 
function titledraw()
    spr(192, 12,20, 13, 4)
    print ("press start!",43,64,9)
end
 
function leveloverdraw()
 rectfill(0,60,128,75,0)
 print("press start to continue",player.x - 40, player.y + 12, 6)
 print("stage clear!",player.x - 18 , player.y , 7)
end
 
function _draw()
if (mode == "title") then
    titledraw()
elseif (mode == "game") then
    gamedraw()
elseif (mode == "levelover") then
    leveloverdraw()
    end
 
end
 
