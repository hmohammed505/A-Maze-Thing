function enemy_x(enemy)
  local lx=enemy.x -- last x
  local ly=enemy.y -- last y
 
  
  if (enemy.x>player.x) then
      enemy.x-=enemy.speed
  elseif (enemy.x<player.x) then
      enemy.x+=enemy.speed
  end
 
  if(cmap(enemy))then
  enemy.x=lx
   
		end
end
 
function enemy_y(enemy)
 local lx=enemy.x -- last x
 local ly=enemy.y -- last y
   
 if (enemy.y>player.y) then
     enemy.y-=enemy.speed
 elseif (enemy.y<player.y) then
     enemy.y+=enemy.speed
 end
   
 if(cmap(enemy))then enemy.y=ly
 end
end


 
--- levelchange ----
 
function solid_tile(x,y)
 
 local tilex = ((x - (x % 8)) / 8)
 local tiley = ((y - (y % 8)) / 8)
 
    if(fget(mget(tilex, tiley), 1))
     then
            return true
                else
            return false
    end
   
end
 
 
--- game over ---

 function chest_tile(x,y)
 
 local tilex = ((x - (x % 8)) / 8)
 local tiley = ((y - (y % 8)) / 8)
 
    if(fget(mget(tilex, tiley), 2))
     then
            return true
                else
            return false
    end
   
end
 
 ----death----
 
 function death()

    if (player.x==enemy11.x and player.y==enemy11.y) then
     return true
     else
     return false
    end
 
end
---- movement ----
function cmap(o)
local ct=false
local cb=false
 
-- if colliding with map tiles
if(o.cm) then
local x1=o.x/8
local y1=o.y/8
local x2=(o.x+7)/8
local y2=(o.y+7)/8
local a=fget(mget(x1,y1),0)
local b=fget(mget(x1,y2),0)
local c=fget(mget(x2,y2),0)
local d=fget(mget(x2,y1),0)
ct=a or b or c or d
end
 
return ct or cb
end
 
 
function move(o)
local lx=o.x -- last x
local ly=o.y -- last y
 
 if (o.x>8*96) then
    if(btn(1)) then
  o.x-=o.speed
  end
  if(btn(3)) then
  o.x+=o.speed
  end
 else
  if(btn(0)) then
  o.x-=o.speed
  end
  if(btn(1)) then
  o.x+=o.speed
  end
 end
 
-- collision, move back
if(cmap(o)) then
o.x=lx
end
end
 
function movey(o)
local lx=o.x -- last x
local ly=o.y -- last y
 
    if (o.x>8*96) then
    if(btn(0)) then
  o.y-=o.speed
  end
  if(btn(2)) then
  o.y+=o.speed
  end
 else
  if(btn(2)) then
  o.y-=o.speed
  end
  if(btn(3)) then
  o.y+=o.speed
  end
 end
 
-- collision, move back
if(cmap(o)) then o.y=ly
 
end
end
